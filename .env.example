# Tenable API Configuration
TENABLE_ACCESS_KEY=your_access_key_here
TENABLE_SECRET_KEY=your_secret_key_here
TENABLE_URL=https://cloud.tenable.com

# Cribl HEC Configuration
CRIBL_HEC_HOST=your_cribl_host_here
CRIBL_HEC_PORT=8088
CRIBL_HEC_TOKEN=your_hec_token_here
CRIBL_HEC_SSL_VERIFY=false

# HEC Batch Processing
HEC_BATCH_SIZE=10000

# Event Limits
MAX_EVENTS_PER_FEED=0

# Checkpoint Configuration
CHECKPOINT_DIR=checkpoints
CHECKPOINT_MAX_IDS=100000
CHECKPOINT_RETENTION_DAYS=30

# Process Lock Configuration
LOCK_DIR=locks
LOCK_TIMEOUT=600

# Deleted Asset Scan Configuration
DELETED_ASSET_SCAN_INTERVAL_HOURS=168

# Script Configuration
LOG_LEVEL=INFO
SCAN_INTERVAL=3600
