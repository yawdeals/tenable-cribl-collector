# Tenable API Configuration
TENABLE_ACCESS_KEY=
TENABLE_SECRET_KEY=
TENABLE_URL=https://cloud.tenable.com

# Cribl HEC Configuration
CRIBL_HEC_HOST=your_cribl_host
CRIBL_HEC_PORT=8088
CRIBL_HEC_TOKEN=your_hec_token_here
CRIBL_HEC_SSL_VERIFY=true

# Redis Configuration (Free/Local Redis - No Password Required)
# For local Redis installation, leave REDIS_PASSWORD empty or commented out
# Redis password is only needed for remote/managed Redis instances
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
# REDIS_PASSWORD=  # Leave empty for free/local Redis
REDIS_KEY_PREFIX=tenable:checkpoint:

# Script Configuration
LOG_LEVEL=INFO
SCAN_INTERVAL=3600
